{
  "name": "swivel5-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "./node_modules/.bin/cross-env NODE_ENV=development webpack -p --progress --watch --config webpack.config.babel.js",
    "server": "export NODE_ENV=development node server.js",
    "build": "./node_modules/.bin/cross-env NODE_ENV=production webpack -p --progress --config webpack.config.babel.js",
    "forever": "./node_modules/.bin/forever start server.js",
    "prod": "npm run build && ./node_modules/.bin/forever start server.js",
    "test-debug": "BABEL_ENV=test node --inspect-brk ./node_modules/.bin/jest --runInBand --env=jsdom",
    "test": "BABEL_ENV=test jest --env=jsdom"
  },
  "repository": "https://gg_thanhnt@bitbucket.org/toancauxanh/swivel5-fe.git",
  "author": "toancauxanh",
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "enzyme-to-json": "^3.2.0",
    "expect": "^1.20.2",
    "express": "^4.15.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "import-export": "^1.0.1",
    "jest": "^22.0.0",
    "jest-cli": "^22.0.0",
    "node-sass": "^4.5.2",
    "react-test-renderer": "^16.0.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-logger": "^3.0.6",
    "rewire": "^3.0.2",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-stage-3": "^6.24.1",
    "bluebird": "^3.5.0",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.6",
    "jquery": "^3.2.1",
    "memory-cache": "^0.1.6",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-cookie": "1.0.4",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-axios-middleware": "^4.0.0",
    "redux-connect": "^7.0.0",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.5.0",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/(*.)(spec|test).js?(x)",
      "**/tests/**/(*.)(spec|test).js?(x)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/__mock__/files.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "./src/setupTests.js"
  }
}
